{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "ait-bus-v3",
	"main": "./.output/server/index.mjs",
	"compatibility_date": "2025-09-27",
	"assets": {
		"binding": "ASSETS",
		"directory": "./.output/public/"
	},
	"observability": {
		"enabled": true
	},
	"compatibility_flags": [
		"nodejs_compat"
	],
	"routes": [
		{
			"custom_domain": true,
			"pattern": "v3.bus.kajilab.net",
		},
		// {
		// 	"custom_domain": true,
		// 	"pattern": "bus.kajilab.net",
		// },
	],
}
